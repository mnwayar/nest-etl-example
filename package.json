{
  "name": "nest-etl-example",
  "version": "1.0.0",
  "description": "ETL test project with NestJS + TypeORM + Postgres",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run start:api",
    "start:api": "NODE_ENV=local nest start api",
    "start:api:dev": "NODE_ENV=local nest start api --watch -r tsconfig-paths/register",
    "start:etl": "NODE_ENV=local nest start etl-runner",
    "start:etl:dev": "NODE_ENV=local nest start etl-runner --watch -r tsconfig-paths/register",
    "build": "nest build api && nest build etl-runner",
    "build:api": "nest build api",
    "build:etl": "nest build etl-runner",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infrastructure/persistence/typeorm/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate src/infrastructure/persistence/typeorm/migrations/auto-migration",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "etl": "node dist/apps/etl-runner/src/main.js",
    "etl:companies": "node dist/apps/etl-runner/src/main.js companies",
    "etl:companies:deleted": "node dist/apps/etl-runner/src/main.js companies:deleted",
    "etl:contacts": "node dist/apps/etl-runner/src/main.js contacts",
    "etl:contacts:deleted": "node dist/apps/etl-runner/src/main.js contacts:deleted",
    "etl:deals": "node dist/apps/etl-runner/src/main.js deals",
    "etl:deals:deleted": "node dist/apps/etl-runner/src/main.js deals:deleted",
    "etl:associations:contacts": "node dist/apps/etl-runner/src/main.js associations:contacts"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.2",
    "@nestjs/typeorm": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  }
}
